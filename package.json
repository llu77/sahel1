    "deploy:cloudflare": "npm run build && wrangler pages publish .next --project-name=gasah",
    "deploy:preview": "npm run build && wrangler pages publish .next --project-name=gasah --branch=preview",
    "db:migrate": "node migrations/migrate-json-to-db.js",
    "db:init": "wrangler d1 execute gasah-db --file=./migrations/init.sql",
    "cf:pages-login": "wrangler login",
